        {
            "name": "GET State - Single Outlet",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://{{microserviceURL}}/{{protocol}}|{{username}}:{{password}}@{{deviceIP}}:{{devicePort}}/state/{{outletNumber}}",
                    "protocol": "http",
                    "host": [
                        "{{microserviceURL}}"
                    ],
                    "path": [
                        "{{protocol}}|{{username}}:{{password}}@{{deviceIP}}:{{devicePort}}",
                        "state",
                        "{{outletNumber}}"
                    ]
                },
                "description": "Gets the state of a single outlet on the APC PDU. The outlet number is accepted in the path. Returns \"on\", \"off\", or \"unknown\".\n\nExample outlet numbers: 1, 2, 3, etc."
            },
